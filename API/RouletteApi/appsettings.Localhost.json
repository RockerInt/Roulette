{
  "Serilog": {
    "Region": "us-east-2",
    "LogGroup": "API",
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    },
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "RententionPolicy": 5
  },
  "Redis": {
    "AllowAdmin": true,
    "EndPoints": [
      {
        "host": "localhost",
        "port": 6379
      }
    ]
  }
}
